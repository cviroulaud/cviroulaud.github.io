def get_matchs(arbre: list) -> list:
    """
    huiti√®me de finale

    Args:
        arbre (list): tableau du tournoi

    Returns:
        list: tableau de tuples
    """
    matchs = []
    i = i_feuille_gauche(arbre)
    while i < len(arbre):
        matchs.append((arbre[i], arbre[i+1]))
        i = i+2
    return matchs
