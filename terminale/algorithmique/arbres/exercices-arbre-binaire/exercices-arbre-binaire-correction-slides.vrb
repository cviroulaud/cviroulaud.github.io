def prefixe_2(self, position: int) -> list:
    if position < len(self.arbre) and \
                    self.arbre[position] is not None:
        return [self.arbre[position]] + \
                    self.prefixe_2(2*position+1) + \
                    self.prefixe_2(2*position+2)
    else:  # cas limite
        return []
\end{lstlisting}
