def tri_bulles(tab: list) -> None:
    for i in range(len(tab)):
        # on s'arrête avant la partie déjà triée
        for j in range(1, len(tab)-i):
            # remonte l'élément le plus grand
            if tab[j-1] > tab[j]:
                echanger(tab, j-1, j)
