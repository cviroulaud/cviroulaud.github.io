def afficher(self) -> str:
    nom_mois = ["janvier", "février", "mars", "avril", "mai", "juin", "juillet", "août", "septembre", "octobre", "novembre", "décembre"]
    return f"{self.jour} / {nom_mois[self.mois - 1]} / {self.annee}"

