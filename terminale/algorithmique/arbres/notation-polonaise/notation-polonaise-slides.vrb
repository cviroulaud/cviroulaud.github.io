\begin{lstlisting}[language=Python , basicstyle=\ttfamily\small, xleftmargin=1em, xrightmargin=1em]
# notation polonaise
préfixe  ['×', 2, '+', 3, 4]

# notation usuelle
infixe  [2, '×', 3, '+', 4]

